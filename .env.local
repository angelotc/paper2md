# Environment variables for paper2md
# Copy this file to .env and fill in your values

# OpenAI API key for LLM-based summarization (REQUIRED)
OPENAI_API_KEY=

# OpenAI model to use (defaults to gpt-5-mini-2025-08-07)
OPENAI_MODEL=gpt-5-mini-2025-08-07

# OpenAI base URL (optional, for using alternative providers)
# Examples:
#   OpenRouter: https://openrouter.ai/api/v1
#   Gemini: https://generativelanguage.googleapis.com/v1beta/openai/
#   Default: https://api.openai.com/v1
OPENAI_BASE_URL=
